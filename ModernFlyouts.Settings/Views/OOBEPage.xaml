    xmlns:icons="using:Fluent.Icons">
    <Grid x:Name="Container">
        <Rectangle x:Name="hostForVisual" Fill="Transparent"/>
        <StackPanel HorizontalAlignment="Center" Canvas.ZIndex="2"  VerticalAlignment="Center">
            <controls:DropShadowPanel Canvas.ZIndex="2" BlurRadius="16" ShadowOpacity="0.12" OffsetY="24" OffsetX="24" CornerRadius="8">
                <FlipView
                    x:Name="FlappyView"
            RequiresPointer="Never"
            BorderBrush="{ThemeResource BorderHighContrastBrush}"
            BorderThickness="3"
            Margin="20"
            Background="{ThemeResource SortHvidBrush}" VerticalAlignment="Stretch"
            MaxHeight="480" HorizontalAlignment="Stretch" MaxWidth="800" 
            CornerRadius="8" 
            Style="{StaticResource FlipStyle}">

                    <FlipViewItem>
                        <Grid>

                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" CornerRadius="8">
                                <Image Source="ms-appx:///Assets/Hero.png" Stretch="UniformToFill" Width="365" Visibility="{x:Bind IsMobile, Mode=OneWay}"/>

                                <StackPanel Orientation="Vertical" Margin="28,40,28,0" Width="375">
                                    <TextBlock Text="Welcome to ModernFlyouts v11 pre-alpha beta indev canary!" FontSize="32" Margin="0,0,0,12" FontWeight="SemiBold" CharacterSpacing="-20"/>
                                    <TextBlock Opacity="0.70" LineHeight="24" Width="325" HorizontalAlignment="Left"
                            Text="Thank you for downloading our app. After following this quick slideshow, you'll know some of the amazing features this app has to offer and be able to customize your experience!" TextWrapping="WrapWholeWords"/>
                                    <StackPanel Orientation="Horizontal" Margin="0,172,0,0" >
                                        <Button Content="Release Notes" Visibility="Collapsed" Background="Transparent" IsEnabled="False" BorderThickness="0.5" BorderBrush="{ThemeResource HvidSortBrush}" Padding="16,8,16,8"/>
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </FlipViewItem>

                    <FlipViewItem>
                        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" CornerRadius="8">
                            <Image Source="ms-appx:///Assets/Hero.png" Stretch="UniformToFill" Width="365" Visibility="{x:Bind IsMobile, Mode=OneWay}"/>

                            <StackPanel Orientation="Vertical" Margin="28,48,28,0" Width="375">
                                <icons:FluentSymbolIcon Symbol="Speaker124" HorizontalAlignment="Left" Margin="0,0,0,12" RenderTransformOrigin="0.5,0.5">
                                    <icons:FluentSymbolIcon.RenderTransform>
                                        <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                    </icons:FluentSymbolIcon.RenderTransform>
                                </icons:FluentSymbolIcon>
                                <TextBlock Text="Lorem ipsum" FontSize="32" Margin="0,0,0,12" FontWeight="SemiBold" CharacterSpacing="-20" TextWrapping="WrapWholeWords" Width="350" HorizontalAlignment="Left"/>
                                <TextBlock Opacity="0.70" LineHeight="24" Width="325" HorizontalAlignment="Left"
                            Text="Lorem ipsum" TextWrapping="WrapWholeWords" />

                            </StackPanel>
                        </StackPanel>
                    </FlipViewItem>
                    <FlipViewItem>

                        <StackPanel Orientation="Vertical" Margin="28,120,28,0" Width="375" HorizontalAlignment="Center">
                            <FontIcon Glyph="&#xE001;" FontSize="40" Foreground="{ThemeResource ResponsiveAccentBrush1}" HorizontalAlignment="Center" Margin="0,0,0,16" />
                            <TextBlock Text="You're all set!" FontSize="32" Margin="0,0,0,12" FontWeight="SemiBold" CharacterSpacing="-20" HorizontalAlignment="Center"/>
                            <TextBlock Opacity="0.70" LineHeight="24" Width="325" HorizontalAlignment="Center" TextAlignment="Center"
                            Text="Lorem ipsum" TextWrapping="WrapWholeWords" />
                            <Button HorizontalAlignment="Center" Padding="16,10,16,10" Style="{StaticResource AccentButtonStyle}" Margin="0, 40,0,0" Click="SkipButton_Click">
                                <Button.Content >
                                    <StackPanel Orientation="Horizontal">
                                        <FontIcon Glyph="&#xE0AB;" Margin="0,0,12,0" FontSize="14"/>
                                        <TextBlock Text="Start using ModernFlyouts"/>
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                        </StackPanel>

                    </FlipViewItem>
                </FlipView>
            </controls:DropShadowPanel>
            <StackPanel x:Name="Stepper">
                <TextBlock HorizontalAlignment="Center" x:Name="Section" Margin="0, 20, 0, 12" FontWeight="SemiBold">OOBE Lorem ipsum</TextBlock>
