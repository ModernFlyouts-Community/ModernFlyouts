                    <muxc:ProgressBar x:Name="bar1" Width="100" Minimum="0" Maximum="2" Value="{x:Bind FlappyView.SelectedIndex, Mode=OneWay}" Margin="4, 0, 4, 0"/>
                    <TextBlock HorizontalAlignment="Center" x:Name="Steps">Step lorem/ipsum</TextBlock>
                </StackPanel>
            </StackPanel>
        </StackPanel>
        <Button Canvas.ZIndex="2" x:Name="Skipper" Background="Transparent" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="34" 
