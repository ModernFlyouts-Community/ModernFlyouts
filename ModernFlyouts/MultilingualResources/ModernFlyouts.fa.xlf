          <source>Insert Mode</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">درج حالت</target>
        </trans-unit>
        <trans-unit id="LockKeysFlyoutHelper.KeyIsOff" translate="yes" xml:space="preserve">
          <source>{0} is off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">{0} خاموش است</target>
          <note from="MultilingualBuild" annotates="source" priority="2">E.g. CapsLock is off</note>
        </trans-unit>
        <trans-unit id="LockKeysFlyoutHelper.KeyIsOn" translate="yes" xml:space="preserve">
          <source>{0} is on</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">{0} در است</target>
          <note from="MultilingualBuild" annotates="source" priority="2">E.g. CapsLock is on</note>
        </trans-unit>
