          <source>About</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">درباره</target>
        </trans-unit>
        <trans-unit id="AirplaneMode.NotAvailable" translate="yes" xml:space="preserve">
          <source>Airplane Mode is not available :(</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">حالت هواپیما در دسترس نیست :(</target>
        </trans-unit>
        <trans-unit id="AirplaneModeOff" translate="yes" xml:space="preserve">
          <source>Airplane mode off</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">‏‏حالت هواپیما خاموش است</target>
        </trans-unit>
        <trans-unit id="AirplaneModeOn" translate="yes" xml:space="preserve">
          <source>Airplane mode on</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">‏‏حالت هواپیما روشن است</target>
        </trans-unit>
