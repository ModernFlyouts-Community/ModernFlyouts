          <source>Open settings window</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">باز کردن پنجره تنظیمات</target>
        </trans-unit>
        <trans-unit id="UnpinTopBar" translate="yes" xml:space="preserve">
          <source>Unpin TopBar</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">باز کردن TopBar</target>
        </trans-unit>
