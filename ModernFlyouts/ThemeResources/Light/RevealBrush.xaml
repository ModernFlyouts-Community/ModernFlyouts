<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ui="http://schemas.modernwpf.com/2019"
                    xmlns:media="clr-namespace:ModernFlyouts.UI.Fluent.Media">

    <Thickness x:Key="AppBarEllipsisButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="AppBarButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="AppBarToggleButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="ButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="RepeatButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="ToggleButtonRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="ListViewItemRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="GridViewItemRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="ComboBoxItemRevealBorderThemeThickness">1</Thickness>
    <Thickness x:Key="ComboBoxItemRevealThemePadding">10,4,10,6</Thickness>
    <Thickness x:Key="ComboBoxItemRevealThemeTouchPadding">10,10,10,12</Thickness>
    <Thickness x:Key="ComboBoxItemRevealThemeGameControllerPadding">10,10,10,12</Thickness>

    <!-- Temporary -->
    <Color x:Key="SystemAltHighColor">#FFFFFFFF</Color>
    <Color x:Key="SystemBaseLowColor">#33000000</Color>
    <Color x:Key="SystemBaseMediumLowColor">#66000000</Color>
    <SolidColorBrush x:Key="SystemControlBackgroundBaseLowBrush" Color="{ui:StaticColor SystemBaseLowColor}" />
    <SolidColorBrush x:Key="SystemControlDisabledBaseMediumLowBrush" Color="{ui:StaticColor SystemBaseMediumLowColor}" />
    <SolidColorBrush x:Key="SystemControlDisabledTransparentBrush" Color="Transparent" />

    <!-- These values are copied from generic.xaml and adjusted by 10% -->
    <Color x:Key="SystemRevealAltHighColor">#E6FFFFFF</Color>
    <Color x:Key="SystemRevealAltLowColor">#2EFFFFFF</Color>
    <Color x:Key="SystemRevealAltMediumColor">#8AFFFFFF</Color>
    <Color x:Key="SystemRevealAltMediumHighColor">#B8FFFFFF</Color>
    <Color x:Key="SystemRevealAltMediumLowColor">#61FFFFFF</Color>
    <Color x:Key="SystemRevealBaseHighColor">#E6000000</Color>
    <Color x:Key="SystemRevealBaseLowColor">#2E000000</Color>
    <Color x:Key="SystemRevealBaseMediumColor">#8A000000</Color>
    <Color x:Key="SystemRevealBaseMediumHighColor">#B8000000</Color>
    <Color x:Key="SystemRevealBaseMediumLowColor">#61000000</Color>
    <Color x:Key="SystemRevealChromeAltLowColor">#E6171717</Color>
    <Color x:Key="SystemRevealChromeBlackHighColor">#E6000000</Color>
    <Color x:Key="SystemRevealChromeBlackLowColor">#2E000000</Color>
    <Color x:Key="SystemRevealChromeBlackMediumLowColor">#66000000</Color>
    <Color x:Key="SystemRevealChromeBlackMediumColor">#B8000000</Color>
    <Color x:Key="SystemRevealChromeHighColor">#E6CCCCCC</Color>
    <Color x:Key="SystemRevealChromeLowColor">#E6F2F2F2</Color>
    <Color x:Key="SystemRevealChromeMediumColor">#E6E6E6E6</Color>
    <Color x:Key="SystemRevealChromeMediumLowColor">#E6F2F2F2</Color>
    <Color x:Key="SystemRevealChromeWhiteColor">#E6FFFFFF</Color>
    <Color x:Key="SystemRevealChromeGrayColor">#E6767676</Color>
    <Color x:Key="SystemRevealListLowColor">#17000000</Color>
    <Color x:Key="SystemRevealListMediumColor">#2E000000</Color>

    <!-- Background Brushes -->
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseLowRevealBackgroundBrush" Color="{ui:StaticColor SystemRevealBaseLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlTransparentRevealBackgroundBrush" Color="Transparent" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlBackgroundAltHighRevealBackgroundBrush" Color="{ui:StaticColor SystemAltHighColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightAccentRevealBackgroundBrush" Color="{ui:DynamicColor SystemAccentColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightAccent3RevealBackgroundBrush" Color="{ui:DynamicColor SystemAccentColorLight3}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightAccent2RevealBackgroundBrush" Color="{ui:DynamicColor SystemAccentColorLight2}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightListMediumRevealBackgroundBrush" Color="{ui:StaticColor SystemRevealListMediumColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightListLowRevealBackgroundBrush" Color="{ui:StaticColor SystemRevealListLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseMediumLowRevealBaseLowBackgroundBrush" Color="{ui:StaticColor SystemRevealBaseLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightBaseMediumLowRevealAccentBackgroundBrush" Color="{ui:DynamicColor SystemAccentColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightListMediumRevealListLowBackgroundBrush" Color="{ui:StaticColor SystemRevealListLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBackgroundRevealBrush="True"
                     x:Key="SystemControlHighlightAccent3RevealAccent2BackgroundBrush" Color="{ui:DynamicColor SystemAccentColorLight2}" />

    <!-- Border Brushes -->
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundAccentRevealBorderBrush" Color="{ui:DynamicColor SystemAccentColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseHighRevealBorderBrush" Color="{ui:StaticColor SystemRevealBaseHighColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseLowRevealBorderBrush" Color="{ui:StaticColor SystemRevealBaseLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseMediumRevealBorderBrush" Color="{ui:StaticColor SystemRevealBaseMediumColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseMediumHighRevealBorderBrush" Color="{ui:StaticColor SystemRevealBaseMediumHighColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundBaseMediumLowRevealBorderBrush" Color="{ui:StaticColor SystemRevealBaseMediumLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundChromeBlackHighRevealBorderBrush" Color="{ui:StaticColor SystemRevealChromeBlackHighColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundChromeBlackMediumRevealChromeBorderBrush" Color="{ui:StaticColor SystemRevealChromeBlackMediumColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundChromeMediumRevealBorderBrush" Color="{ui:StaticColor SystemRevealChromeMediumColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundChromeMediumLowRevealBorderBrush" Color="{ui:StaticColor SystemRevealChromeMediumLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundChromeWhiteRevealBorderBrush" Color="{ui:StaticColor SystemRevealChromeWhiteColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlTransparentRevealListLowBorderBrush" Color="Transparent" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundListLowRevealBorderBrush" Color="{ui:StaticColor SystemRevealListLowColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlBackgroundListMediumRevealBorderBrush" Color="{ui:StaticColor SystemRevealListMediumColor}" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
                     x:Key="SystemControlTransparentRevealBorderBrush" Color="Transparent" />
    <SolidColorBrush media:RevealBrushHelper.IsBorderRevealBrush="True"
